<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pye.project.controller.CancelReservationController">
   <children>
      <Label fx:id="cancelResLabel" layoutX="155.0" layoutY="111.0" text="Are you sure you would like to cancel your reservation?" />
      <Button fx:id="cancelButton" layoutX="232.0" layoutY="160.0" mnemonicParsing="false" onAction="#cancelButtonAction" text="Yes, Please Cancel." />
      <Button fx:id="noButton" layoutX="282.0" layoutY="200.0" mnemonicParsing="false" onAction="#noButtonAction" text="No" />
   </children>
</AnchorPane>
